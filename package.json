{
  "name": "study-material-app",
  "version": "1.0.0",
  "description": "Personal study material management system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "install-client": "cd client && npm install",
    "build": "npm run install-client && cd client && npm run build",
    "render-build": "npm install && npm run install-client && cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "study",
    "material",
    "mern",
    "education"
  ],
  "author": "Sameer Kushwaha",
  "license": "MIT"
}
